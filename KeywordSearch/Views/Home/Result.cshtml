@using KeywordSearch.Models
@model Search

@{
    ViewBag.Title = "Result";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Result</h2>

@{  
    foreach (int index in ViewBag.resultIndex as List<int>)
    {
        <span>
            Page: @Model.Results[index].Page &nbsp;&nbsp;
        </span>
        
        <span>
            Index: @index
        </span>
        <br />
    }
}
